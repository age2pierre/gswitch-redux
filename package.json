{
  "name": "gswitch-like",
  "version": "0.1.0",
  "description": "A little game to try out react-three-fiber",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/age2pierre/gswitch-like.git"
  },
  "license": "ISC",
  "main": "src/index.tsx",
  "scripts": {
    "build": "npm run clean && parcel build src/index.html --experimental-scope-hoisting",
    "clean": "rimraf dist .cache",
    "format": "prettier --write 'src/**/*.{ts,tsx,json}'",
    "lint": "tslint -c tslint.json --fix 'src/**/*.{ts,tsx}'",
    "start": "parcel serve src/index.html --open --no-autoinstall",
    "test": "jest",
    "upgrade-interactive": "npx npm-check --update"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "dependencies": {
    "@blueprintjs/core": "3.21.0",
    "detect-collisions": "2.4.4",
    "immer": "5.0.0",
    "lodash-es": "4.17.15",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2",
    "react-spring": "8.0.27",
    "react-three-fiber": "3.0.15",
    "three": "0.110.0",
    "ts-keycode-enum": "1.0.6",
    "utility-types": "3.10.0",
    "zustand": "2.2.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "@types/lodash-es": "4.17.3",
    "@types/moment-timezone": "0.5.12",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "5.1.2",
    "autoprefixer": "9.7.2",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lint-staged": "9.4.3",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-bundle-visualiser": "1.2.0",
    "parcel-plugin-static-files-copy": "2.2.1",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "sass": "1.23.7",
    "sort-package-json": "1.24.0",
    "ts-jest": "24.2.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-no-focused-test": "0.5.0",
    "tslint-react": "4.1.0",
    "tslint-react-hooks": "2.2.1",
    "typescript": "3.7.2"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "src/assets/static",
        "staticOutDir": "static"
      }
    ]
  }
}
