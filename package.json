{
  "name": "gswitch-like",
  "version": "0.1.0",
  "description": "A little game to try out react-three-fiber",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/age2pierre/gswitch-like.git"
  },
  "license": "ISC",
  "main": "src/index.tsx",
  "scripts": {
    "build": "npm run clean && parcel build src/index.html --experimental-scope-hoisting",
    "clean": "rimraf dist .cache",
    "lint": "tslint -c tslint.json --fix 'src/**/*.{ts,tsx}'",
    "start": "parcel serve src/index.html --open --no-autoinstall",
    "test": "jest",
    "upgrade-interactive": "npm-check --update"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "dependencies": {
    "@material-ui/core": "4.6.0",
    "@material-ui/icons": "4.5.1",
    "detect-collisions": "2.4.4",
    "immer": "5.0.0",
    "lodash-es": "4.17.15",
    "parcel-bundler": "1.12.4",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-three-fiber": "3.0.11",
    "three": "0.110.0",
    "ts-keycode-enum": "1.0.6",
    "zustand": "2.2.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.22",
    "@types/lodash-es": "4.17.3",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "autoprefixer": "9.7.1",
    "fbx2gltf": "^0.9.7",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "npm-check": "5.9.0",
    "prettier": "^1.19.1",
    "rimraf": "3.0.0",
    "sass": "1.23.3",
    "sort-package-json": "1.23.1",
    "ts-jest": "24.1.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-no-focused-test": "0.5.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "4.1.0",
    "tslint-react-hooks": "2.2.1",
    "typescript": "3.7.2"
  }
}
