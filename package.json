{
  "name": "gswitch-redux",
  "version": "0.1.0",
  "description": "A little game to try out react-three-fiber",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/age2pierre/gswitch-like.git"
  },
  "license": "ISC",
  "main": "src/index.tsx",
  "scripts": {
    "build": "parcel build src/index.html",
    "format": "prettier --write 'src/**/*.{ts,tsx,json}'",
    "lint": "tslint -c tslint.json --fix 'src/**/*.{ts,tsx}'",
    "start": "parcel serve src/index.html --open --no-autoinstall",
    "test": "jest",
    "upgrade-interactive": "npx npm-check --update"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "dependencies": {
    "@blueprintjs/core": "3.21.0",
    "@blueprintjs/icons": "3.13.0",
    "detect-collisions": "2.4.4",
    "immer": "5.3.6",
    "lodash-es": "4.17.15",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2",
    "react-spring": "8.0.27",
    "react-three-fiber": "4.0.17",
    "scheduler": "0.18.0",
    "three": "0.113.2",
    "ts-keycode-enum": "1.0.6",
    "utility-types": "3.10.0",
    "zustand": "2.2.3"
  },
  "devDependencies": {
    "@blueprintjs/tslint-config": "2.0.0",
    "@types/jest": "25.1.3",
    "@types/lodash-es": "4.17.3",
    "@types/moment-timezone": "0.5.12",
    "@types/react": "16.9.21",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "5.1.3",
    "@types/scheduler": "0.16.1",
    "autoprefixer": "9.7.4",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "lint-staged": "10.0.7",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-bundle-visualiser": "1.2.0",
    "parcel-plugin-static-files-copy": "2.3.1",
    "prettier": "1.19.1",
    "sass": "1.25.0",
    "sort-package-json": "1.40.0",
    "ts-jest": "25.2.1",
    "tslint": "6.0.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-no-focused-test": "0.5.0",
    "tslint-react": "4.2.0",
    "tslint-react-hooks": "2.2.1",
    "typescript": "3.7.5"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "src/assets/static",
        "staticOutDir": "static"
      }
    ]
  }
}
