{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-react-hooks",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rulesDirectory": ["tslint-no-focused-test"],
  "rules": {
    "prettier": true,
    "react-hooks-nesting": "error",
    "no-submodule-imports": false,
    "interface-name": false,
    "jsx-no-lambda": false
  }
}
